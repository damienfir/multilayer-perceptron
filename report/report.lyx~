\begin_layout Subsection
Binary MLP
\end_layout

\begin_layout Paragraph
Backpropagation
\end_layout

\begin_layout Standard
The vectorized backpropagation equations for the binary MLP are as follows:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
g^{\left(3\right)} & = & r^{\left(3\right)}z^{\left(2\right)}\left(a^{\left(2\right)}\right)\\
r^{\left(3\right)} & = & \sigma\left(a^{\left(3\right)}\right)-\frac{1}{2}\left(t+1\right)\\
\mathbf{r}_{L}^{\left(2\right)} & = & \mathbf{a}_{LR}^{\left(2\right)}\sigma'\left(\mathbf{a}_{L}^{\left(2\right)}\right)\sigma\left(\mathbf{a}_{R}^{\left(2\right)}\right)w^{\left(3\right)}r^{\left(3\right)}\\
\mathbf{r}_{R}^{\left(2\right)} & = & \mathbf{a}_{LR}^{\left(2\right)}\sigma\left(\mathbf{a}_{L}^{\left(2\right)}\right)\sigma'\left(\mathbf{a}_{R}^{\left(2\right)}\right)w^{\left(3\right)}r^{\left(3\right)}\\
\mathbf{r}_{LR}^{\left(3\right)} & = & \sigma\left(\mathbf{a}_{L}^{\left(2\right)}\right)\sigma\left(\mathbf{a}_{R}^{\left(2\right)}\right)w^{\left(3\right)}r^{\left(3\right)}\\
\mathbf{g}_{L}^{\left(2\right)} & = & \mathbf{r}_{L}^{\left(2\right)}\mathbf{z}_{L}^{\left(1\right)}\left(\mathbf{a}_{L}^{\left(1\right)}\right)\\
\mathbf{g}_{R}^{\left(2\right)} & = & \mathbf{r}_{R}^{\left(2\right)}\mathbf{z}_{R}^{\left(1\right)}\left(\mathbf{a}_{R}^{\left(1\right)}\right)\\
\mathbf{g}_{LR}^{\left(2\right)} & = & \mathbf{r}_{LR}^{\left(2\right)}\left[\mathbf{z}_{L}^{\left(1\right)}\,\mathbf{z}_{R}^{\left(1\right)}\right]\\
\mathbf{r}_{L}^{\left(1\right)} & = & tanh'\left(\mathbf{a}_{L}^{\left(2\right)}\right)\left(\left(\mathbf{w}_{L}^{\left(2\right)}\right)^{T}\mathbf{r}_{L}^{\left(2\right)}+\left(\mathbf{w}_{LR[L]}^{\left(2\right)}\right)^{T}\mathbf{r}_{LR}^{\left(2\right)}\right)\\
\mathbf{r}_{R}^{\left(1\right)} & = & tanh'\left(\mathbf{a}_{R}^{\left(2\right)}\right)\left(\left(\mathbf{w}_{L}^{\left(2\right)}\right)^{T}\mathbf{r}_{L}^{\left(2\right)}+\left(\mathbf{w}_{LR[R]}^{\left(2\right)}\right)^{T}\mathbf{r}_{LR}^{\left(2\right)}\right)\\
\mathbf{g}_{L}^{\left(1\right)} & = & \mathbf{r}_{L}^{\left(1\right)}\mathbf{x}_{L}\\
\mathbf{g}_{R}^{\left(1\right)} & = & \mathbf{r_{R}^{\left(1\right)}}\mathbf{x}_{R}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
with 
\begin_inset Formula $w_{LR[L]}^{\left(2\right)}$
\end_inset

 representing the portion of the weight matrix corresponding to the left
 image, and analogously for 
\begin_inset Formula $w_{LR[R]}^{\left(2\right)}$
\end_inset

.
 The notation 
\begin_inset Formula $\left[\mathbf{z}_{L}^{\left(1\right)}\,\mathbf{z}_{R}^{\left(1\right)}\right]$
\end_inset

 means the concatenation of 
\begin_inset Formula $\mathbf{z}_{L}^{\left(1\right)}$
\end_inset

 and 
\begin_inset Formula $\mathbf{z}_{R}^{\left(1\right)}$
\end_inset


\end_layout

